            `responsedelay`,
            `downloaddelay`,
            `httpsize`,
            `httpstatus`,
            `tcpcount`,
            `tcphead`,
            `tcpconn`,
            `tcpretrans`,
            `l2delay`,
            `l2loss`,
            `l2trace`,
            `l2tracedelay`,
            `lanhoops`,
            `landelay`,
            `sharepc`,
            `wanip`,
            `lanip`,
            `sysbacknetwork`,
            `syscpuusage`,
            `syscpu`,
